<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="css/quizzHome.css">

</head>
<body>

    <h3>Welcome , <%- user.name %></h3>


<div class="quiz-container">
    <% quizzs.forEach(function(quizz) { %>
      <div class="quiz-block" >
        <h2><%= quizz.quizzTitleName %></h2>   
        <form action="/takeQuizz?quizzId=<%- quizz._id %>" method="post">            
            <!-- <input type="hidden" value="<%- quizz._id %>" name="quizzId">                         -->
            <input type="submit" class="take-quiz-btn" value="Take Quizz">
        </form>     
      </div>
    <% }); %>

    <div class="container">
      <img src="img/profile.svg" alt="" class="icon">
    </div>
    

</div>


</body>
</html>

  